"*":
  core:
    disabledPackages: [
      "markdown-preview"
      "markdown-preview-plus-opener"
    ]
    telemetryConsent: "limited"
    themes: [
      "one-light-ui"
      "one-light-syntax"
    ]
  "exception-reporting":
    userId: "42ba6965-72ff-4dcb-a579-8e2c6e21fa80"
  "markdown-preview-plus":
    mathConfig:
      enableLatexRenderingByDefault: true
      latexRenderer: "HTML-CSS"
    previewConfig:
      activatePreviewWithEditor: true
    renderer: "pandoc"
    syncConfig:
      syncPreviewOnChange: true
      syncPreviewOnEditorScroll: true
    useGitHubStyle: true
  "markdown-preview-plus-opener":
    closePreviewWhenClosingEditor: true
  "package-sync":
    createOnChange: true
    forceOverwrite: true
  welcome:
    showOnStartup: false
